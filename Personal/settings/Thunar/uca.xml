<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open in Terminal</name>
	<submenu></submenu>
	<unique-id>1488955845049151-1</unique-id>
	<command>alacritty</command>
	<description>Open Terminal here</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>system-file-manager</icon>
	<name>Open folder as root</name>
	<submenu></submenu>
	<unique-id>1493475601060449-3</unique-id>
	<command>pkexec thunar %f</command>
	<description>Open Thunar as ROOT</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>meld</icon>
	<name>Compare with Meld</name>
	<submenu></submenu>
	<unique-id>1492232362549799-1</unique-id>
	<command>meld %F</command>
	<description>Compare selected files or directories</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>catfish</icon>
	<name>Search with Catfish</name>
	<submenu></submenu>
	<unique-id>1489089852658523-2</unique-id>
	<command>catfish %f</command>
	<description>Search files with catfish</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>system-reboot</icon>
	<name>Convert PNG to JPG</name>
	<submenu></submenu>
	<unique-id>1510662686010963-1</unique-id>
	<command>mogrify -format jpg %N .png</command>
	<description>Converts PNG images to JPG format</description>
	<range></range>
	<patterns>*.png</patterns>
	<image-files/>
</action>
<action>
	<icon>system-run</icon>
	<name>Make Executable</name>
	<submenu></submenu>
	<unique-id>1526670239648084-2</unique-id>
	<command>chmod +x %F</command>
	<description>Makes the selected file(s) executable</description>
	<range></range>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>code-context</icon>
	<name>Copy File Content to Clipboard</name>
	<submenu></submenu>
	<unique-id>1693673897756105-1</unique-id>
	<command>xclip -selection clipboard %f</command>
	<description>Copies the content of the selected file to the clipboard</description>
	<range>*</range>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>draw-path</icon>
	<name>Copy Path to Clipboard</name>
	<submenu></submenu>
	<unique-id>1693674597522528-1</unique-id>
	<command>echo -n %f | xclip -selection &quot;clipboard&quot;</command>
	<description>Copies the path name of the selected file or directory to the clipboard</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>system-users</icon>
	<name>Ownership to User</name>
	<submenu></submenu>
	<unique-id>1693675385597370-1</unique-id>
	<command>pkexec chown $(id -u):$(id -g) %F</command>
	<description>Changes ownership of the selected files or directories to user</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>gksu</icon>
	<name>Ownership to ROOT</name>
	<submenu></submenu>
	<unique-id>1693675724291859-1</unique-id>
	<command>pkexec chown root:root %F</command>
	<description>Changes ownership of the selected files or directories to root</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>vlc</icon>
	<name>Add to VLC Playlist</name>
	<submenu></submenu>
	<unique-id>1696214381330166-1</unique-id>
	<command>/usr/bin/vlc --one-instance --playlist-enqueue %F</command>
	<description>Add the currently selected file(s) to the VLC mediaplayer&apos;s playlist</description>
	<range>*</range>
	<patterns>*</patterns>
	<audio-files/>
	<video-files/>
</action>
</actions>
